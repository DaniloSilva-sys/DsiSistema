<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <meta name="description" content="" />
    <meta name="author" content="" />
    <title>@ViewBag.Title - DsiSistemas</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body id="page-top">
    <nav class="navbar navbar-expand navbar-dark bg-orange static-top">
        <a class="navbar-brand mr-1" href="~/Views/Home/Index.cshtml"><img src="img/logo.png" width="180"></a>
        <button class="btn btn-link btn-sm text-white order-1 order-sm-0" id="sidebarToggle" href="#">
            <i class="fas fa-bars"></i>
        </button>
    </nav>
    <div id="wrapper">
        <ul id="menu" class="sidebar navbar-nav">
            <li class="nav-item active">
                <a class="nav-link" href="~/Views/Home/Index.cshtml">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Dashboard</span>
                </a>
            </li>
            <li id="menu_li" class="nav-item">
                <a id="foodtrucks" class="nav-link" href="foodtruck-manager.html">
                    <i class="fas fa-fw fa-chart-area"></i>
                    <span>Food Trucks</span>
                </a>
            </li>
            <li class="nav-item">
                <a id="products" class="nav-link" href="product-manager.html">
                    <i class="fas fa-fw fa-table"></i>
                    <span>Produtos</span>
                </a>
            </li>
            <li class="nav-item">
                <a id="sales" class="nav-link" href="sales.html">
                    <i class="fas fa-fw fa-table"></i>
                    <span>Vendas</span>
                </a>
            </li>
            <li class="nav-item">
                <a id="charts" class="nav-link" href="charts.html">
                    <i class="fas fa-fw fa-table"></i>
                    <span>Relatórios</span>
                </a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="login.html">
                    <i class="fas fa-fw fa-table"></i>
                    <span>Sair</span>
                </a>
            </li>
        </ul>

        <div id="content-wrapper">
            <div class="container-fluid">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">
                        <a href="#">Dashboard</a>
                    </li>
                    <li class="breadcrumb-item active">Dashboard</li>
                </ol>
            </div>
            @RenderBody()
            <hr />
            @*<body>
            
        </body>*@
            <footer class="sticky-footer">
                <div class="container my-auto">
                    <center>
                        <img class="icone" src="img/facebook1.png" width="50" />
                        <img class="icone" src="img/twitter1.png" width="50" />
                        <img class="icone" src="img/instagram1.png" width="50" />
                        <img class="icone" src="img/snap1.png" width="50" />
                        <img class="icone" src="img/whats.png" width="50" />
                    </center>
                    <div class="copyright text-center my-auto">
                        <span>Copyright © Food Truck Manager 2019</span>
                    </div>
                </div>
            </footer>
        </div>
    </div>
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Logout Modal-->
    <div class="modal fade"
         id="logoutModal"
         tabindex="-1"
         role="dialog"
         aria-labelledby="exampleModalLabel"
         aria-hidden="true">
    </div>

    <script>
    window.fcWidget.init({     /*aqui é a instalçao freshchat*/
            token: "38095888-436b-48c8-a268-fae170ed89af",
            host: "https://wchat.freshchat.com"
        });</script>

    <!--verificando se está logado e se é admin-->
    <script src="js/check-logged-adm.js"></script>

    <!-- Bootstrap core JavaScript-->
    <script src="libs/jquery/jquery.min.js"></script>
    <script src="libs/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="libs/jquery-easing/jquery.easing.min.js"></script>

    <!-- Page level plugin JavaScript-->
    <script src="libs/chart.js/Chart.min.js"></script>
    <script src="libs/datatables/jquery.dataTables.js"></script>
    <script src="libs/datatables/dataTables.bootstrap4.js"></script>

    <!-- Custom scripts for all pages-->
    <script src="js/sb-admin.min.js"></script>

    <!-- Demo scripts for this page-->
    <script src="js/demo/datatables-demo.js"></script>
    <script src="js/demo/chart-area-demo.js"></script>
</body>
